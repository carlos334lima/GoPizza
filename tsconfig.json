{
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "@Assets/*":["./src/Assets/*"],
      "@Screens/*":["./src/Screens/*"],
      "@Theme/*":["./src/Theme/*"],
      "@Hooks/*":["./src/Hooks/*"],
      "@Components/*":["./src/Components/*"],
      "@Utils/*":["./src/Utils/*"],
      "@Types/*":["./src/@Types/*"],
    },
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "isolatedModules": true,
    "jsx": "react-native",
    "lib": ["es2017"],
    "moduleResolution": "node",
    "noEmit": true,
    "strict": true,
    "target": "esnext"
  },
  "exclude": [
    "node_modules",
    "babel.config.js",
    "metro.config.js",
    "jest.config.js"
  ]
}
